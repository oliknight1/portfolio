{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "description": {
      "type": "text"
    },
    "technologies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::technology.technology"
    },
    "subheading": {
      "type": "string"
    },
    "github_link": {
      "type": "string"
    },
    "live_link": {
      "type": "string"
    },
    "project_click_link": {
      "type": "string",
      "required": true
    },
    "is_cold_boot": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
